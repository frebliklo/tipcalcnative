{
  "expo": {
    "name": "TipCalc",
    "description": "Calculate the tip you need to pay your for service in the US",
    "slug": "tipcalcnative",
    "privacy": "public",
    "githubUrl": "https://github.com/frebliklo/tipcalcnative",
    "sdkVersion": "29.0.0",
    "platforms": ["ios", "android"],
    "ios": {
      "bundleIdentifier": "com.frebliklo.tipcalc",
      "supportsTablet": false,
      "icon": "./assets/icon-ios.png"
    },
    "android": {
      "package": "com.frebliklo.tipcalc",
      "versionCode": 3,
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon-white.png",
        "backgroundColor": "#151F8C",
        "backgroundImage": "./assets/adaptive-bg.png"
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION"
      ]
    },
    "version": "1.3.0",
    "primaryColor": "#151F8C",
    "orientation": "portrait",
    "icon": "./assets/icon-color.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "updates": {
      "enabled": false,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 300000
    },
    "assetBundlePatterns": [
      "assets/*",
      "assets/icons/*"
    ]
  }
}
